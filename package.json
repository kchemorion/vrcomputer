{
  "name": "aframe-in-app-browser",
  "version": "0.0.5",
  "description": "A progressive in-app browser for aframe using an electron based headless browser companion app.",
  "homepage": "https://github.com/shaneharris/aframe-in-app-browser",
  "main": "server.js",
  "keywords": [
    "AFRAME",
    "Browser",
    "Electron"
  ],
  "scripts": {
    "start": "concurrently \"webpack-dev-server --mode development --port 8899\" \"npm run start-electron\"",
    "start-electron": "electron .",
    "build": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaneharris/aframe-in-app-browser.git"
  },
  "bugs": {
    "url": "https://github.com/shaneharris/aframe-in-app-browser/issues"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "electron": "^27.0.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "author": "Shane Harris",
  "license": "MIT"
}
